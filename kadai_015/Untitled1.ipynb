      "source": [
        "class Human:\n",
        "    def __init__(self, name, age):\n",
        "        self.name = name\n",
        "        self.age = age\n",
        "\n",
        "    def printinfo(self):\n",
        "        print(f\"名前: {self.name}, 年齢: {self.age}\")\n",
        "\n",
        "\n",
        "person1 = Human(\"太郎\", 25)\n",
        "person2 = Human(\"花子\", 30)\n",
        "\n",
        "person1.printinfo()\n",
        "person2.printinfo()\n",
        "\n",
        "\n"
      ]
    }
